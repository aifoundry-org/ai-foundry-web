    },
    "company": {
      "type": "string"
    },
    "featuring": {
      "displayName": "featuring",
      "type": "component",
      "repeatable": true,
      "component": "customer-post.featuring"
    },
    "industry": {
      "type": "string"
    },
    "challenge": {
      "type": "text"
    },
    "impact": {
      "type": "text"
    },
    "description": {
      "type": "text"
    }
